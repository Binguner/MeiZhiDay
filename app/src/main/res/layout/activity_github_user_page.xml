    <android.support.design.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <android.support.design.widget.AppBarLayout
            android:id="@+id/github_page_appbarlayout1"
            android:layout_width="match_parent"
            android:layout_height="226dp"
            android:fitsSystemWindows="true">
            <android.support.design.widget.CollapsingToolbarLayout
                android:id="@+id/githubt_page_collaspingToolbar"
                android:layout_width="match_parent"
                app:contentScrim="@color/colorToolbar"
                android:layout_height="match_parent"
                android:fitsSystemWindows="true"
                app:layout_scrollFlags="scroll|exitUntilCollapsed|snap">
                <ImageView
                    app:layout_collapseMode="parallax"
                    app:layout_collapseParallaxMultiplier="0.7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@mipmap/gituserbg1"
                    android:scaleType="centerCrop"/>
                <android.support.v7.widget.Toolbar
                    android:layout_width="match_parent"
                    app:layout_collapseMode="pin"
                    android:gravity="top"
                    android:layout_height="96dp"
                    >
                    <RelativeLayout
                        android:gravity="top"
                        app:contentInsetStartWithNavigation="0dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageView
                            app:contentInsetStartWithNavigation="0dp"
                            android:layout_marginLeft="-12dp"
                            android:id="@+id/github_page_back"
                            android:layout_width="40dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/back_24_white"/>

                        <ImageView
                            android:layout_marginRight="10dp"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/white_person_24"/>
                    </RelativeLayout>


                </android.support.v7.widget.Toolbar>
                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_marginTop="70dp"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:id="@+id/github_circleImageView1"
                    app:layout_collapseMode="parallax"
                    app:layout_collapseParallaxMultiplier="0.7"
                    android:layout_gravity="center_horizontal"
                    android:src="@mipmap/bg2"/>


                <TextView
                    android:id="@+id/github_desc"
                    android:layout_marginBottom="50dp"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="@color/colorWhite"
                    android:layout_width="wrap_content"
                    app:layout_collapseMode="parallax"
                    app:layout_collapseParallaxMultiplier="0.7"
                    android:layout_height="wrap_content"
                    android:text="I can't go on,I will go on"
                    android:singleLine="true"
                    android:textSize="15dp"/>

                <android.support.design.widget.TabLayout
                    android:id="@+id/github_tablelayout1"
                    android:layout_gravity="bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:tabTextColor="@color/colorWhite"
                    app:tabIndicatorColor="@color/colorWhite">
                    <android.support.design.widget.TabItem
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="WatchEvents"
                        />
                    <android.support.design.widget.TabItem
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="MyEvents"/>
                    <android.support.design.widget.TabItem
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Repos"/>

                </android.support.design.widget.TabLayout>




            </android.support.design.widget.CollapsingToolbarLayout>

        </android.support.design.widget.AppBarLayout>

      <!--  <android.support.v4.widget.NestedScrollView
